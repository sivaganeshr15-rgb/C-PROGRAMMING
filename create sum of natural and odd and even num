#include <stdio.h>

int main() {
    int limit, i = 1;
    long long sum_natural = 0; // Use long long for larger sums
    long long sum_odd = 0;
    long long sum_even = 0;

    printf("Enter the upper limit: ");
    scanf("%d", &limit);

    while (i <= limit) {
        sum_natural += i; // Add to natural number sum

        if (i % 2 == 0) {
            sum_even += i; // Add to even number sum
        } else {
            sum_odd += i; // Add to odd number sum
        }
        i++;
    }

    printf("Sum of natural numbers up to %d: %lld\n", limit, sum_natural);
    printf("Sum of odd numbers up to %d: %lld\n", limit, sum_odd);
    printf("Sum of even numbers up to %d: %lld\n", limit, sum_even);

    return 0;
}
